<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    Action created for  product-->
    <record id="action_inventory_digital_product" model="ir.actions.act_window">
        <field name="name">Digital Product</field>
        <field name="type">ir.actions.act_window</field>    
        <field name="res_model">inventory.product</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_product_category':'digital'}</field>
        <field name="domain">[('product_category','=','digital')]</field>
    </record>
    <menuitem
            id="menu_inventory_digital_product"
            name="Digtal Products"
            parent="menu_inventory_product_master"
            action="action_inventory_digital_product"
    />

</odoo>